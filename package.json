{
  "name": "plain-chart",
  "version": "0.1.0-alpha.0",
  "description": "一个比 Apache ECharts 更简单的开源可视化图表库",
  "main": "dist/plain-chart.min.js",
  "scripts": {
    "start": "npm install && npm run dev",
    "dev": "rollup --config rollup.config.js -i ./src/index.js -o ./dist/plain-chart.js --watch",
    "build": "nodejs -d dist && rollup --config rollup.config.js && terser ./dist/plain-chart.js -o ./dist/plain-chart.min.js && nodejs --config nodejs.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/clunch-contrib/chart.git"
  },
  "keywords": [
    "chart",
    "clunch",
    "easy"
  ],
  "author": "你好2007",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/clunch-contrib/chart/issues"
  },
  "homepage": "https://clunch-contrib.github.io/chart/",
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@hai2007/nodejs": "^2.0.1",
    "@rollup/plugin-babel": "^5.2.1",
    "rollup": "^1.17.0",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "terser": "^4.1.2"
  },
  "dependencies": {
    "@hai2007/algorithm": "^0.3.0",
    "@hai2007/tool": "^0.9.0"
  }
}
